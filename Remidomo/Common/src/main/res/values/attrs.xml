<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomEditTextPreference">
        <attr name="validator_ip" format="boolean"/>
        <attr name="validator_int" format="boolean"/>
        <attr name="min_integer" format="integer"/>
        <attr name="max_integer" format="integer"/>
    </declare-styleable>

    <declare-styleable name="CustomTimePickerPreference">
        <attr name="defaultValue" format="integer"/>
    </declare-styleable>

    <declare-styleable name="CustomSpinnerPreference">
        <attr name="defaultVal" format="integer"/>
        <attr name="min" format="integer"/>
        <attr name="max" format="integer"/>
    </declare-styleable>

    <declare-styleable name="CustomSpinner">
        <attr name="linked_edit_text" format="reference"/>
    </declare-styleable>
</resources>